<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">

        <shape
            android:padding="10dp"
            android:shape="rectangle" >

            <corners
                android:radius="10dp"
                />

            <solid android:color="#FFFFFFFF" />

            <stroke
                android:width="2sp"
                android:color="@color/original_primary" />

        </shape>



    </item>


    <item android:state_enabled="false">

        <shape
            android:padding="10dp"
            android:shape="rectangle" >

            <corners
                android:radius="10dp"
                />

            <solid android:color="@color/original_white" />

            <stroke android:color="@color/grey_400"
                android:width="2sp"/>


        </shape>



    </item>

</selector>